      "start_id": {
        "type": "integer",
        "description": "Start ID (Optional)",
        "pattern": "^[0-9]*$",
        "examples": []
      },
      "url_base": {
        "type": "integer",
        "description": "URL to the KYVE Chain API.",
        "examples": []
      }
    }
  }
}